@{
    ViewBag.Title = "صفحه اصلی";
    Layout = "~/Areas/Website/Views/Shared/_Layout.cshtml";
}

<section id="main-slider" class="no-margin">
    <div class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#main-slider" data-slide-to="0" class="active"></li>
            <li data-target="#main-slider" data-slide-to="1"></li>
            <li data-target="#main-slider" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner carousel-fade">
            <div class="item active" style="background-image:url(../../images/slider/bg1.JPG)">
                <div class="container">
                    <div class="row slide-margin">
                        <div class="col-sm-5 col-sm-offset-1">
                            <div class="carousel-content" style="margin-top:220px">
                                <h1 class="animation animated-item-1">گالری نگاه نو</h1>  
                                <h2 class="animation animated-item-2">پذیرای آثار هنری شما</h2>
                            </div>
                        </div>
                        @*<div class="col-sm-6 hidden-xs animation animated-item-4">
                            <div class="slider-img">
                                <img src="~/images/slider/img2.png" class="img-responsive">
                            </div>
                        </div>*@
                    </div>
                </div>
            </div><!--/.item-->

            <div class="item" style="background-image:url(../../images/slider/bg2.JPG)">
                <div class="container">
                    <div class="row slide-margin">
                        <div class="col-sm-6">
                            <div class="carousel-content">
                                <h1 class="animation animated-item-1">برگزاری نمایشگاه های فردی و گروهی</h1>
                            </div>
                        </div>
                        @*<div class="col-sm-6 hidden-xs animation animated-item-4">
                            <div class="slider-img">
                                <img src="~/images/slider/img2.png" class="img-responsive">
                            </div>
                        </div>*@
                    </div>
                </div>
            </div><!--/.item-->

            <div class="item" style="background-image:url(../../images/slider/bg3.JPG)">
                <div class="container">
                    <div class="row slide-margin">
                        <div class="col-sm-6">
                            <div class="carousel-content">
                                <h1 class="animation animated-item-1">محیطی آرام و متفاوت</h1>
                            </div>
                        </div>
                        @*<div class="col-sm-6 hidden-xs animation animated-item-4">
                        <div class="slider-img">
                            <img src="~/images/slider/img2.png" class="img-responsive">
                        </div>
                    </div>*@
                    </div>
                </div>
            </div><!--/.item-->
        </div><!--/.carousel-inner-->
    </div><!--/.carousel-->
    <a class="prev hidden-xs" href="#main-slider" data-slide="prev">
        <i class="fa fa-chevron-left"></i>
    </a>
    <a class="next hidden-xs" href="#main-slider" data-slide="next">
        <i class="fa fa-chevron-right"></i>
    </a>
</section><!--/#main-slider-->
<section id="feature">
    <div class="container">
        <div class="center wow fadeInDown">
            <h2>خدمات گالری نگاه نو</h2>
            <p class="lead">گالری نگاه در زمینه های متفاوتی فعالیت می نماید:</p>
        </div>
        <div class="row">
            <div class="features">
                <div class="col-sm-6 wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
                    <div class="feature-wrap">
                        <div class="col-sm-3">
                            <img src="~/images/sale.png" />
                        </div>
                        <div class="col-sm-9">
                            <h2>همکاری در فروش</h2>
                            <h3> در راستای کمک به هنرمندان کشور، گالری نگاه نو در فروش آثار هنرمندان با آن ها همکاری می نماید.</h3>
                        </div>
                    </div>
                </div><!--/.col-md-6-->

                <div class="col-sm-6 wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
                    <div class="feature-wrap">
                        @*<i class="fa fa-bullhorn"></i>*@
                        <div class="col-sm-3">
                            <img src="~/images/art1.png" />
                        </div>
                        <div class="col-sm-9">
                            <h2>برگزاری نمایشگاه</h2>
                            <h3>ما مفتخریم تا پذیرای آثار هنری شما در گالری نگاه نو،در محیطی مدرن و دلپذیر،  باشیم.</h3>
                        </div>
                    </div>
                </div><!--/.col-md-6-->
               
               
                <div class="col-sm-6 wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
                    <div class="feature-wrap">
                        <div class="col-sm-3">
                            <img src="~/images/workshop.png" />
                        </div>
                        <div class="col-sm-9">
                            <h2>برگزاری ورک شاپ های آموزشی</h2>
                            <h3>با همکاری اساتید مجرب کشور، گالری نگاه نو کارگاه های آموزشی در زمینه های مختلف هنری برگزار می نماید</h3>
                        </div>
                    </div>
                </div><!--/.col-md-6-->
              
              <div class="col-sm-6 wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
                    <div class="feature-wrap">
                        <div class="col-sm-3">
                            <img src="~/images/prize.png" />
                        </div>
                        <div class="col-sm-9">
                            <h2>برگزاری جشنواره هایی در رشته های هنر های تجسمی </h2>
                            <h3>از دیگر فعالیت های ما برگزاری جشواره های هنری و برگزیدن نفرات برتر توسط داوران و معرفی هنرمندان جدید به جامعه هنری کشور است.</h3>
                        </div>
                    </div>
                </div><!--/.col-md-6-->
               
            </div><!--/.services-->
        </div><!--/.row-->
    </div><!--/.container-->
</section><!--/#feature-->

<section id="content">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 wow fadeInDown">
                <div class="tab-wrap">
                    <div class="media">
                        <div class="parrent pull-left">
                            <ul class="nav nav-tabs nav-stacked">
                                <li class="active"><a href="#tab1" data-toggle="tab" class="analistic-01">برنامه هفته جاری</a></li>
                                <li ><a href="#tab2" data-toggle="tab" class="analistic-02">برنامه هفته آینده</a></li>
                                <li class=""><a href="#tab3" data-toggle="tab" class="tehnical">برنامه دو هفته بعدی</a></li>
                                <li class=""><a href="#tab4" data-toggle="tab" class="tehnical">برنامه سه هفته بعدی</a></li>
                            </ul>
                        </div>
                        <div class="parrent media-body">
                            <div class="tab-content">
                                <div class="tab-pane fade active in" id="tab1">
                                    <div class="media">
                                        <div class="pull-left">
                                            <img class="img-responsive" src="~/images/programs/current.jpg">
                                        </div>
                                        <div class="media-body">
                                            <h2>نمایشگاه ...</h2>
                                            <h3>تاریخ و ساعت</h3>
                                            <p>شرح نمایشگاه</p>
                                        </div>
                                    </div>
                                </div>

                                <div class="tab-pane fade" id="tab2">
                                    <div class="media">
                                        <div class="pull-left">
                                            <img class="img-responsive" src="~/images/programs/next.jpg">
                                        </div>
                                        <div class="media-body">
                                            <h2>نمایشگاه ...</h2>
                                            <h3>تاریخ و ساعت</h3>
                                            <p>شرح نمایشگاه</p>
                                        </div>
                                    </div>
                                </div>

                                <div class="tab-pane fade" id="tab3">
                                    <div class="media">
                                        <div class="pull-left">
                                            <img class="img-responsive" src="~/images/programs/next2.jpg">
                                        </div>
                                        <div class="media-body">
                                            <h2>نمایشگاه ...</h2>
                                            <h3>تاریخ و ساعت</h3>
                                            <p>شرح نمایشگاه</p>
                                        </div>
                                    </div>
                                </div>

                                <div class="tab-pane fade" id="tab4">
                                    <div class="media">
                                        <div class="pull-left">
                                            <img class="img-responsive" src="~/images/programs/next3.jpg">
                                        </div>
                                        <div class="media-body">
                                            <h2>نمایشگاه ...</h2>
                                            <h3>تاریخ و ساعت</h3>
                                            <p>شرح نمایشگاه</p>
                                        </div>
                                    </div>
                                </div>

                            </div> <!--/.tab-content-->
                        </div> <!--/.media-body-->
                    </div> <!--/.media-->
                </div><!--/.tab-wrap-->
            </div><!--/.col-sm-6-->
        </div><!--/.row-->
    </div><!--/.container-->
</section><!--/#content-->

